        <script><![CDATA[var scu = new SSPChartUtils();

// Get a list of all departments
var depts = ['ALL'];
var gr = new GlideRecord('cmn_department');
// gr.addEncodedQuery('u_active=true');
gr.orderBy('name');
gr.query();

while (gr.next()) {
	depts.push(gr.getDisplayValue('name'));
}

// Generate cache data for each 
// department, including "ALL"
for (x = 0; x < depts.length; x++) {
	scu.genCacheDataForDept(depts[x]);	
}
]]></script>
